          "rawSql": "SELECT \n  dc.full_name as \"Customer Name\",\n  dc.customer_segment as \"Segment\",\n  dc.city as \"City\",\n  COUNT(fs.order_id) as \"Total Orders\",\n  SUM(fs.total_amount) as \"Total Spent\"\nFROM target_data.dim_customers dc\nJOIN target_data.fact_sales fs ON dc.customer_key = fs.customer_key\nGROUP BY dc.customer_key, dc.full_name, dc.customer_segment, dc.city\nORDER BY \"Total Spent\" DESC\nLIMIT 10",
